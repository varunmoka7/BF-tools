{
  "name": "waste-intelligence-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:safe": "./dev-server.sh",
    "dev:clean": "./dev-server.sh --clean",
    "monitor": "./monitor-server.sh",
    "process-manager": "./process-manager.sh",
    "status": "./infrastructure-status.sh",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf .next .swc node_modules/.cache .turbo",
    "kill-servers": "pkill -f 'next-server' || pkill -f 'next dev' || lsof -ti:3000 | xargs kill -9 || echo 'No servers to kill'"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
