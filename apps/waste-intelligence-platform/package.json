    "@types/leaflet": "^1.9.20",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.16",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.441.0",
    "next": "14.2.16",
    "papaparse": "^5.4.1",
    "postcss": "^8",
    "react": "^18",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18",
    "react-leaflet": "^4.2.1",
    "recharts": "^2.15.4",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
