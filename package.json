{
  "name": "bf-tools",
  "version": "1.0.0",
  "description": "Waste management - GHG Emissions integration and solutions",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "cd apps/waste-intelligence-platform && npm run dev",
    "build": "cd apps/waste-intelligence-platform && npm run build",
    "start": "cd apps/waste-intelligence-platform && npm run start",
    "lint": "cd apps/waste-intelligence-platform && npm run lint",
    "test": "cd apps/waste-intelligence-platform && npm run test",
    "backend:dev": "nodemon --config backend/config/nodemon.json backend/src/app.ts",
    "backend:build": "tsc -p backend/config/tsconfig.json",
    "backend:test": "jest --config backend/config/jest.config.js",
    "unified:dev": "ts-node src/unified-server.ts",
    "unified:build": "npm run build && ts-node src/unified-server.ts",
    "migrate:supabase": "node scripts/migrate-to-supabase.js",
    "setup:supabase": "node scripts/setup-supabase-schema.js",
    "sync:waste-data": "node scripts/sync-waste-data.js",
    "migrate:auth": "ts-node scripts/run-auth-migration.ts",
    "auth:cleanup-sessions": "ts-node scripts/cleanup-auth-sessions.ts",
    "auth:security-report": "ts-node scripts/auth-security-report.ts",
    "security:config-check": "node scripts/security-config-check.js",
    "security:monitoring": "node scripts/security-monitoring.js",
    "health:check": "node scripts/health-check.js",
    "test:security": "jest --config jest.config.security.js",
    "test:auth:unit": "jest --config jest.config.security.js --testPathPattern=auth.*unit",
    "test:auth:integration": "jest --config jest.config.security.js --testPathPattern=auth.*integration",
    "test:security:e2e": "playwright test --config playwright.config.security.ts",
    "docker:build": "docker build -t waste-intelligence:latest .",
    "docker:dev": "docker-compose -f docker-compose.yml up -d",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "start:test": "NODE_ENV=test npm run build && NODE_ENV=test npm start",
    "setup:test-db": "node scripts/setup-test-database.js",
    "backup:db": "docker-compose run --rm db-backup",
    "security:scan": "npm audit && snyk test",
    "security:fix": "npm audit fix && snyk fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/varunmoka7/BF-tools.git"
  },
  "keywords": [
    "waste-management",
    "circular-economy",
    "sustainability",
    "api",
    "analytics"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@supabase/supabase-js": "^2.55.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/leaflet": "^1.9.20",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/jsonwebtoken": "^9.0.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.0",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.539.0",
    "morgan": "^1.10.1",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.8.1",
    "recharts": "^3.1.2",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "uuid": "^11.1.0",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.3",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.2.0",
    "ip": "^2.0.1",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.2.1",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "shadcn": "^3.2.1",
    "tailwindcss": "^4.1.11",
    "@playwright/test": "^1.40.0",
    "jest-junit": "^16.0.0",
    "jest-html-reporters": "^3.1.5",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "@types/jest": "^29.5.8",
    "eslint-plugin-security": "^1.7.1",
    "snyk": "^1.1266.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": {
    "url": "https://github.com/varunmoka7/BF-tools/issues"
  },
  "homepage": "https://github.com/varunmoka7/BF-tools#readme"
}
